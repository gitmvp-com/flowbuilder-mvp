{
  "name": "flowbuilder-mvp",
  "version": "0.1.0",
  "description": "Simple workflow automation MVP inspired by n8n",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:server": "tsc && tsc-alias",
    "build:client": "vite build",
    "start": "node dist/server/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "workflow",
    "automation",
    "n8n",
    "mvp"
  ],
  "engines": {
    "node": ">=20.19"
  },
  "dependencies": {
    "express": "^5.1.0",
    "axios": "1.12.0",
    "cors": "^2.8.5",
    "uuid": "10.0.0",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.50",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.3",
    "typescript": "5.9.2",
    "vite": "^6.0.7"
  }
}